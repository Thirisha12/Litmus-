{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Description of my extension.",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webNavigation"
  ],
 
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker":"background.js"
  }
}
